        <div class="bloque-contenedor">
            <div class="row hedeExperiencia">
                <p class="sans text-uppercase pull-left" style="font-weight: 500; color: #262626; font-size: 16px;"><strong>Certificacion</strong></p>

            </div>
            <!-- Contenido Aqui -->
            <div class="experiencia " id="contenidoCerti" >
                <div id="consultaCertificacion" class="experiencias">
                    {%if Certificacion|default is not empty %}
                        {% for datos in Certificacion %}
                            <div class="row " id="Cert{{datos.id}}">
