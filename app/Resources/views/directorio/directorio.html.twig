                                $('#filtro').text($('#searchBox').val() + '| ' + $('#searchInputDept').val());
                            }
                            if ($('#searchInputDept').val() !== "" && $('#searchBox').val() == "")
                            {
                                $('#filtro').text($('#searchInputDept').val());
                            }
                            if ($('#searchInputDept').val() == "" && $('#searchBox').val() !== "")
                            {
                                $('#filtro').text($('#searchBox').val());
                            }

                            $('.totalRegistros').html(data.numRegistros);
                            $('.filtroRegistros').html(data.filtroRegistros);

                            if (data.data.length != 0) {
                                $('.numRegistros,.anteriorMov,.siguienteMov,.numeroPaginas').show();
                                $('#inicioInput').val((parseInt($('#inicioInput').val()) + 1));
