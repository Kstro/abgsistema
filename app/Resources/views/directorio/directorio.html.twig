                if (busqueda == "" || busqueda != "") {

                    $.ajax({
                        data: {inicio: inicio, longitud: longitud, paginaActual: paginaActual, busqueda: busqueda, munId: munId, orderBy: orderBy},
                        url: "{{path('busqueda_data')}}",
