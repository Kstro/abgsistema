                                            cadena = '<div class=\"item-directorio-movil\" style=\"margin-top: 0px !important;\">';
                                        }
                                        else{
                                            cadena = '<div class=\"item-directorio-movil\" style=\"margin-top: -1px !important;\">';
                                        }
                                            
                                        
