                                    var busquedaTelefono = "#busquedaTelefono" + (i + 1);
                                    var busquedaEspecialidades = "#busquedaEspecialidades" + (i + 1);

                                    var busquedaNombre = data.data[i].nombres + ' ' + data.data[i].apellido;
                                    var busquedaCargo = "";
                                    var telefonos = "";
                                    var busquedaCorreo = data.data[i].correoelectronico;
                                    var busquedaFoto = data.data[i].foto;

                                    var busquedaSitioWeb = data.data[i].sitioweb;
                                    
                                    if (busquedaFoto == null) {
                                        busquedaFoto = "";
                                    }

                                    if (data.data[i].puesto == null) {
                                        busquedaCargo = "";
                                    } else {
                                        busquedaCargo = data.data[i].puesto;
                                    }

                                    if (busquedaCorreo == null) {
                                        busquedaCorreo = "-";
                                    }

                                    if (busquedaSitioWeb == null) {
                                        busquedaSitioWeb = "";
                                    }

                                    //console.log(data);

                                    var telefonos = "";
                                    if (data.data[i].telefono_fijo === null || data.data[i].telefono_fijo === '') {
                                        //data.data[i].telefonoFijo="";
                                        if (data.data[i].telefono_movil === null || data.data[i].telefono_movil === '') {
                                            telefonos = "-";
                                        } else {
                                            telefonos = "+503 " + data.data[i].telefono_movil;
                                        }
                                    } else {
                                        telefonos = "+503 " + data.data[i].telefono_fijo;
                                        if (data.data[i].telefono_movil === null || data.data[i].telefono_movil === '') {
                                            telefonos += "";
                                        } else {
                                            telefonos += ", +503 " + data.data[i].telefono_movil;
                                        }

                                    }

                                    //console.log(data.data[i].especialidades.length);
                                    var especialidades = "";
                                    if (data.data[i].especialidad !== 'undefined') {
                                        especialidades += data.data[i].especialidad;

                                    } else {
                                        especialidades = "N/A";
                                    }
                                    var verificado = '';
                                    if (data.data[i].verificado == 1) {
                                        verificado = '<img src="{{asset('Resources/src/img/verified.png')}}" style="width: 25px; margin-bottom: 0px;margin-left:1px;" data-toggle="tooltip" data-placement="right" title="Abogado Verificado"></p>';
                                    }

                                    if(!$('#encabezado').is(':visible')){
                                        $('.anteriorMov').show();
                                        $('.siguienteMov').show();
