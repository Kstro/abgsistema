                $('#registros').html('<div style="text-align:center;margin-top:20px;" class="col-md-12 col-xs-12 pull-right"><i class="fa fa-circle-o-notch fa-spin fa-1x fa-fw"></i>Buscando...</div>');
                //return false;
                orderBy=$('#randomDir').val();
                //numero de registros por pÃ¡gina
                if(!$('#encabezado').is(':visible')){
                    longitud=6;//movil
                }
                else{
                    longitud=10;//desktop
                }
                //  var deptoId = $('#filtrarDepartamento').val();
                var munId = $('#searchInputDept').val();
                var cadenaPaginas = "";
