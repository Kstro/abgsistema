{% extends 'base.html.twig' %}

{% block body %}
    <h1>AbgFacturacion</h1>

    <table>
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ abgFacturacion.id }}</td>
            </tr>
            <tr>
                <th>Fechapago</th>
                <td>{% if abgFacturacion.fechaPago %}{{ abgFacturacion.fechaPago|date('Y-m-d') }}{% endif %}</td>
            </tr>
            <tr>
                <th>Monto</th>
                <td>{{ abgFacturacion.monto }}</td>
            </tr>
            <tr>
                <th>Servicio</th>
                <td>{{ abgFacturacion.servicio }}</td>
            </tr>
        </tbody>
    </table>

    <ul>
        <li>
            <a href="{{ path('abgfacturacion_index') }}">Back to the list</a>
        </li>
        <li>
            <a href="{{ path('abgfacturacion_edit', { 'id': abgFacturacion.id }) }}">Edit</a>
        </li>
        <li>
            {{ form_start(delete_form) }}
                <input type="submit" value="Delete">
            {{ form_end(delete_form) }}
        </li>
    </ul>
{% endblock %}
