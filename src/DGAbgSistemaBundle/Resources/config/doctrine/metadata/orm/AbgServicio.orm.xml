<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="AbgServicio" table="abg_servicio">
    <indexes>
      <index name="fk_abg_servicio_abg_persona1_idx" columns="abg_persona_id"/>
      <index name="fk_abg_servicio_ctl_empresa1_idx" columns="ctl_empresa_id"/>
    </indexes>
    <id name="id" type="integer" column="id">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="nombreServicio" type="string" column="nombre_servicio" length="60" nullable="false"/>
    <field name="descripcion" type="string" column="descripcion" length="200" nullable="false"/>
    <many-to-one field="ctlEmpresa" target-entity="CtlEmpresa">
      <join-columns>
        <join-column name="ctl_empresa_id" referenced-column-name="id"/>
      </join-columns>
    </many-to-one>
    <many-to-one field="abgPersona" target-entity="AbgPersona">
      <join-columns>
        <join-column name="abg_persona_id" referenced-column-name="id"/>
      </join-columns>
    </many-to-one>
  </entity>
</doctrine-mapping>
