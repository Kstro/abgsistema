<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="AbgEntrada" table="abg_entrada">
    <indexes>
      <index name="fk_abg_entrada_abg_imagen_blog1_idx" columns="abg_imagen_blog_id"/>
      <index name="fk_abg_entrada_abg_categoria_entrada1_idx" columns="abg_categoria_entrada_id"/>
      <index name="fk_abg_entrada_ctl_usuario1_idx" columns="ctl_usuario_id"/>
    </indexes>
    <id name="id" type="integer" column="id">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="tituloEntrada" type="string" column="titulo_entrada" length="200" nullable="false"/>
    <field name="fecha" type="string" column="fecha" length="45" nullable="false"/>
    <field name="contenido" type="text" column="contenido" length="65535" nullable="false"/>
    <many-to-one field="ctlUsuario" target-entity="CtlUsuario">
      <join-columns>
        <join-column name="ctl_usuario_id" referenced-column-name="id"/>
      </join-columns>
    </many-to-one>
    <many-to-one field="abgCategoriaEntrada" target-entity="CtlCategoriaEntrada">
      <join-columns>
        <join-column name="abg_categoria_entrada_id" referenced-column-name="id"/>
      </join-columns>
    </many-to-one>
    <many-to-one field="abgImagenBlog" target-entity="CtlImagenBlog">
      <join-columns>
        <join-column name="abg_imagen_blog_id" referenced-column-name="id"/>
      </join-columns>
    </many-to-one>
  </entity>
</doctrine-mapping>
